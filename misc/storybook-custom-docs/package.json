{
  "name": "@component-controls/storybook-custom-docs",
  "version": "0.6.0",
  "description": "Storybook custom docs pages",
  "keywords": [
    "addon",
    "storybook"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "yarn cross-env NODE_ENV=production rollup -c",
    "dev": "yarn cross-env NODE_ENV=development rollup -cw",
    "docs": "ts-node  -O '{\"module\": \"commonjs\"}' ../../scripts/docs.ts",
    "fix": "yarn lint --fix",
    "lint": "yarn eslint . --ext mdx,ts,tsx",
    "prepare": "yarn build",
    "test": "yarn jest --passWithNoTests"
  },
  "homepage": "https://github.com/ccontrols/component-controls",
  "bugs": {
    "url": "https://github.com/ccontrols/component-controls/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccontrols/component-controls.git",
    "directory": "third-party/storybook-custom-docs"
  },
  "license": "MIT",
  "dependencies": {
    "broadcast-channel": "^3.1.0",
    "core-js": "^3.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@storybook/addons": "next",
    "@storybook/api": "next",
    "@storybook/client-api": "next",
    "@storybook/core-events": "next"
    
  },
  "peerDependencies": {
    "@storybook/addons": "*",
    "@storybook/api": "*",
    "@storybook/client-api": "*",
    "@storybook/core-events":"*",
    "react": "*",
    "react-dom": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "Atanas Stoyanov"
  ],
  "gitHead": "5068cf281e57cce95509b6c7ac5683c8d5618d98"
}
