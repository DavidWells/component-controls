{
  "name": "@component-controls/storybook-custom-docs",
  "version": "1.0.3",
  "description": "Storybook custom docs pages",
  "keywords": [
    "addon",
    "storybook"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "preset.js"
  ],
  "scripts": {
    "build": "yarn cross-env NODE_ENV=production rollup -c",
    "dev": "yarn cross-env NODE_ENV=development rollup -cw",
    "docs": "ts-node  -O '{\"module\": \"commonjs\"}' ../../scripts/docs.ts",
    "fix": "yarn lint --fix",
    "lint": "yarn eslint . --ext mdx,ts,tsx",
    "prepare": "yarn build",
    "test": "yarn jest --passWithNoTests"
  },
  "homepage": "https://github.com/ccontrols/component-controls",
  "bugs": {
    "url": "https://github.com/ccontrols/component-controls/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccontrols/component-controls.git",
    "directory": "misc/storybook-custom-docs"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@storybook/addons": "6.0.0-alpha.28",
    "@storybook/api": "6.0.0-alpha.28",
    "@storybook/client-api": "6.0.0-alpha.28",
    "@storybook/core-events": "6.0.0-alpha.28",
    "@types/react": "^16.9.34",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@storybook/addons": "*",
    "@storybook/api": "*",
    "@storybook/client-api": "*",
    "@storybook/core-events": "*",
    "react": "*",
    "react-dom": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "Atanas Stoyanov"
  ],
