{
  "name": "@component-controls/storybook",
  "version": "0.3.4",
  "description": "Storybook Addon For live editing of component controls",
  "keywords": [
    "addon",
    "storybook"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "yarn cross-env NODE_ENV=production rollup -c",
    "dev": "yarn cross-env NODE_ENV=development yarn build -w",
    "fix": "yarn lint --fix",
    "lint": "yarn eslint . --ext mdx,ts,tsx",
    "prepare": "yarn build",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "test": "yarn jest --passWithNoTests"
  },
  "homepage": "https://github.com/ccontrols/component-controls",
  "bugs": {
    "url": "https://github.com/ccontrols/component-controls/issues"
  },
