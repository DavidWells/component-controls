    "@theme-ui/presets": "^0.3.0",
    "copy-to-clipboard": "^3.0.8",
    "core-js": "^3.0.1",
    "global": "^4.3.2",
    "qs": "^6.6.0",
    "react": "^16.12.0"
  },
